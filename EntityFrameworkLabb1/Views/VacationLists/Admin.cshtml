<h4>Sök ledighet efter datum</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AdminViewResults">
            <div class="form-group">
                <label for="StartDate" class="control-label">Start datum</label>
                <input type="date" name="Start" class="form-control" />
            </div>
            <div class="form-group">
                <label for="EndDate" class="control-label">Slut datum</label>
                <input type="date" name="Stop" class="form-control" />
            </div><br/>
            <div class="form-group">
                <input type="submit" value="Sök" class="btn btn-outline-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Tillbaka till listan</a>
</div>
